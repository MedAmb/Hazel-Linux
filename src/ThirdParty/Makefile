export D=1

#==============================================
# TARGETS FOR THIRD PARTY LIBRARIES
#==============================================
spdlog-build:
	$(MAKE) -C spdlog build

spdlog-install:
	$(MAKE) -C spdlog install

spdlog-clobber:
	$(MAKE) -C spdlog clobber

spdlog-uninstall:
	$(MAKE) -C spdlog uninstall

glfw-build:
	$(MAKE) -C glfw build

glfw-install:
	$(MAKE) -C glfw install

glfw-clobber:
	$(MAKE) -C glfw clobber

glfw-uninstall:
	$(MAKE) -C glfw uninstall